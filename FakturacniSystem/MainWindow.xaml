        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FakturacniSystem"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <ListBox x:Name="InvoiceList" d:ItemsSource="{d:SampleData ItemCount=5}" Margin="590,10,10,66"/>
        <Button x:Name="NewInvoiceButton" Content="New Invoice" HorizontalAlignment="Left" Margin="648,373,0,0" VerticalAlignment="Top" Height="32" RenderTransformOrigin="0.498,0.322" Width="84" Click="NewInvoiceButtonClick"/>
    </Grid>
</Window>
